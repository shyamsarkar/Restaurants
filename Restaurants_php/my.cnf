[mysqld]
# Set MySQL character set
#character-set-server = utf8mb4

# Set MySQL collation
#collation-server = utf8mb4_unicode_ci

# Set MySQL data directory
#datadir = /var/lib/mysql

# Set MySQL pid file location
pid-file = /var/lib/mysql/mysql.pid

# Set MySQL log file locations
#general_log_file = /var/log/mysql/mysql.log
#log_error = /var/log/mysql/error.log

# Set MySQL server ID
#server-id = 1

# Set MySQL bind address
#bind-address = 0.0.0.0

# Set MySQL max connections
#max_connections = 100

# Set MySQL max allowed packet size
#max_allowed_packet = 16M

# Set MySQL key buffer size
#key_buffer_size = 16M

# Set MySQL innodb buffer pool size
#innodb_buffer_pool_size = 128M

# Set MySQL innodb log file size
#innodb_log_file_size = 64M

# Set MySQL innodb file per table
#innodb_file_per_table = 1

# Set MySQL innodb flush log at trx commit
#innodb_flush_log_at_trx_commit = 1

# Set MySQL character set settings
#character-set-client-handshake = FALSE
#character-set-server = utf8mb4
#collation-server = utf8mb4_unicode_ci

# Set MySQL replication settings
# Example:
# server-id               = 1
# log_bin                 = /var/log/mysql/mysql-bin.log
# binlog_do_db            = example_db
# relay-log               = /var/log/mysql/mysql-relay-bin
# log_slave_updates       = 1
# auto_increment_increment = 2
# auto_increment_offset  = 1

# Set MySQL replication master settings
# Example:
# binlog_format           = MIXED
# log_slave_updates       = 1
# read_only               = 1
# replicate_do_db         = example_db

# Set MySQL replication slave settings
# Example:
# relay_log               = /var/log/mysql/mysql-relay-bin
# read_only               = 1

# Set MySQL replication group settings
# Example:
# binlog_format           = MIXED
# gtid_mode               = ON
# enforce_gtid_consistency= ON
# master_info_repository  = TABLE
# relay_log_info_repository = TABLE

# Set MySQL SSL/TLS settings
# Example:
# ssl-ca                  = /etc/mysql/ssl/ca-cert.pem
# ssl-cert                = /etc/mysql/ssl/server-cert.pem
# ssl-key                 = /etc/mysql/ssl/server-key.pem

# Set MySQL performance schema settings
# Example:
# performance_schema      = ON

# Set MySQL temporary table settings
# Example:
# tmpdir                  = /var/tmp
